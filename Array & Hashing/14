class Solution:
    def longestCommonPrefix(self, strs: List[str]) -> str:
        ans = ""
        for k in range(len(strs[0])):
            cnt = 0
            for i in range(1,len(strs)):
                if k>=len(strs[i]):
                    break
                elif strs[0][k]==strs[i][k]:
                    cnt+=1
                else:
                    break
            if cnt == len(strs)-1:
                ans+=strs[0][k]
            else:
                break
        return ans
